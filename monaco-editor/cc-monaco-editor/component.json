	  "writeback" : true
    },
	"editorInitialValue" : {
	  "propertyGroup":"common",
      "type" : "string",
	  "description": "The initial value of the editor",
      "displayName": "Initial Value"
    }
  },
  "methods": {

  }, 
  "events": {

  }, 
